{
  "tasks": {
    "lume": "echo \"import 'lume/cli.ts'\" | deno run --unstable -A -",
    "build": "deno task lume",
    "serve": "deno task lume -s"
  },
  "imports": {
    "lume/": "https://deno.land/x/lume@v1.16.0/",
    "html/": "https://deno.land/x/html@v1.2.0/",
    "preact/": "https://esm.sh/preact@10.13.1/",
    "preact/hooks/": "https://unpkg.com/preact@10.13.1/hooks/",
    "@i/": "./_includes/"
  },
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  }
}
